package example;

import java.util.ArrayList;

public class DBManager {
    private ArrayList<Data> list = new ArrayList<Data>();
    public DBManager(){
        Data d1 = new Data("food", "궁물즈짜장볶이", 4, 1);
        Data d2 = new Data("food", "허니버터아몬드130g", 3, 2);
        Data d3 = new Data("food", "와사비맛아몬드130g", 2, 3);
        Data d4 = new Data("food", "궁물즈라뽂이", 2, 3);
        Data d5 = new Data("instant", "휴게소소시지치즈", 2, 1);
        Data d6 = new Data("simple", "빽햄스페셜정식", 3, 1);
        Data d7 = new Data("simple", "오븐에구운트리플치킨", 3, 1);
        Data d8 = new Data("simple", "햄에그샐러드버거", 2, 3);
        Data d9 = new Data("snack", "꿀미니건빵280g", 7, 1);
        Data d10 = new Data("snack", "쫀득롤민트초코", 1, 2);
        
        list.add(d1);
        list.add(d2);
        list.add(d3);
        list.add(d4);
        list.add(d5);
        list.add(d6);
        list.add(d7);
        list.add(d8);
        list.add(d9);
        list.add(d10);
    }
    
    public ArrayList<Data> returnData(){
        return list;
    }
}
